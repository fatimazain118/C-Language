// Write a C program to print all the numbers between 1 to n, that are divisible by either 5 or 7.

#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);

    for (int i = 0; i <= n; i++)
    {
        if (i % 5 == 0 || i % 7 == 0)
        {
            printf("%d ", i);
        }
    }
    return 0;
}
